{
  "name": "blog-site",
  "version": "0.2.1-0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "release": "standard-version && npm publish",
    "release:dry-run": "standard-version --dry-run",
    "precommit": "npm test",
    "prepush": "npm test",
    "postrelease": "git push --follow-tags origin main && gh release create $(git describe --tags $(git rev-list --tags --max-count=1)) --generate-notes",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:patch": "standard-version --release-as patch",
    "prerelease": "standard-version --prerelease"
  },
  "dependencies": {
    "@portabletext/react": "^3.2.0",
    "@sanity/image-url": "^1.1.0",
    "@sanity/vision": "^3.68.3",
    "@supabase/supabase-js": "^2.49.1",
    "blog-site": "file:",
    "lucide-react": "^0.482.0",
    "mongodb": "^6.13.1",
    "next": "15.1.3",
    "next-sanity": "^9.8.30",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "sanity": "^3.68.3",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.3",
    "postcss": "^8",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
